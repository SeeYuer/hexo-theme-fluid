<div class="footer-inner">
  <% if (theme.footer.content) { %>
    <div class="footer-content">
      <%- theme.footer.content %>
    </div>
    <% if(theme.footer.badge.ICP) { %>
      <div><object
          data="https://img.shields.io/badge/%E8%9C%80%20ICP%20%E5%A4%87-2021032128%E5%8F%B7--3-00b6ff?style=flat&logo=data:image/x-icon;base64,&link=https://beian.miit.gov.cn/&color=f65700&labelColor=3993cf"
          style="position: relative;top: 3.5px;margin-right: 5px;"></object></div>
      <% } %>

        <% if(theme.footer.badge.cdn) { %>
          <div>
            <span><object
                data="https://img.shields.io/badge/CDN-%E4%B8%83%E7%89%9B%E4%BA%91-07beff?style=flat&logo=data:image/svg+xml;base64,&labelColor=f2fbff&link=https://www.qiniu.com/"
                style="position: relative;top: 3.5px;margin-right: 5px;"></object></span>
            <span><object
                data="https://img.shields.io/badge/CDN-%E5%8F%88%E6%8B%8D%E4%BA%91-00b6ff?style=flat&logo=data:image/png;base64,&link=https://www.upyun.com/?utm_source=lianmeng&&utm_medium=referral"
                style="position: relative;top: 3.5px;margin-right: 5px;"></object></span>
            <span><object
                data="https://img.shields.io/badge/CDN-jsDelivr-blue?logo=jsDelivr&color=ff5627&labelColor=ffeae4&link=https://www.jsdelivr.com/"
                style="position: relative;top: 3.5px;"></object></span>
          </div>
          <% } %>

            <% if(theme.footer.badge.mirror_site.enable) { %>
              <div>
                <!-- <span style="font-size: 85%;">我的分站：</span> -->
                <span><object
                    data="https://img.shields.io/badge/Mirror%20Site-Vercel-black?logo=Vercel&labelColor=fb6415&link=<%- theme.footer.badge.mirror_site.Vercel %>&logoColor=black"
                    style="position: relative;top: 3.5px;margin-right: 5px;"></object></span>
                <span><object
                    data="https://img.shields.io/badge/Mirror%20Site-Netlify-3dabc4?logo=Netlify&labelColor=inactive&link=<%- theme.footer.badge.mirror_site.Netlify %>"
                    style="position: relative;top: 3.5px;"></object></span>
              </div>
              <% } %>
                <div>
                  <% if(theme.footer.runtime) { %>
                    <span class="runtime" style="margin-right: 5px;">
                      <span class="left"><i class="iconfont icon-shijian"></i> 运行时间</span><span class="right"><span
                          id="timeDate">载入天数...</span><span id="times">载入时分秒...</span></span>
                    </span>
                    <script src="https://cdn1.tianli0.top/gh/SeeYuer/Blog_Static_Flies@main/js/duration.js"></script>
                    <% if(theme.footer.site_wordcount) { %>
                      <span class="site-wordcount">
                        <!-- 两个 span 不要换行 -->
                        <span class="left"><i class="iconfont icon-yongyan"></i> 全站字数</span><span class="right"><%= wordtotal(site) %></span>
                      </span>
                      <% } %>
                    <% } %>
                </div>
                <% } %>
                  <% if (theme.footer.statistics.enable) { %>
                    <%- partial('_partials/footer/statistics.ejs') %>
                      <% } %>
                        <% if(theme.footer.beian.enable) { %>
                          <!-- 备案信息 ICP for China -->
                          <%- partial('_partials/footer/beian.ejs') %>
                            <% } %>
                              <% if(theme.web_analytics.cnzz) { %>
                                <!-- cnzz Analytics Icon -->
                                <span id="cnzz_stat_icon_<%= theme.web_analytics.cnzz %>" style="display: none"></span>
                                <% } %>
</div>